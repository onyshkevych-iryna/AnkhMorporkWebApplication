@using AnkhMorporkWebApp.Models
@model AnkhMorporkWebApp.Models.PlayerBeggarViewModel
@{
    ViewBag.Title = "Index";
}
<p>Current balance:</p> @Model.player.Balance

<h2>@Model.slogan @Model.begar.Fee @Model.slogans</h2>
@Html.ActionLink("Yes", "Yes", "BeggarsGuild", new { sum = Model.begar.Fee, balance = Model.player.Balance }, null)
@Html.ActionLink("No", "No", "BeggarsGuild", Model.player, null)
